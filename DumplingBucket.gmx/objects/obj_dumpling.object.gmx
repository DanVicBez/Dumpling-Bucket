            <string>///Update loop

with (obj_game_over) if (instance_count &gt; 0) exit;

x -= global.dumpling_speed;

var bHalfWidth = obj_bucket.sprite_width / 2,
    bHalfHeight = obj_bucket.sprite_height / 2,
    bX = obj_bucket.x,
    bY = obj_bucket.y;

//If the dumpling collides with the front edge of the bucket
if x                     &lt;= bX + bHalfWidth
&amp;&amp; x + sprite_width / 2  &gt;= bX + bHalfWidth
&amp;&amp; y - sprite_height / 2 &gt;= bY - bHalfHeight
&amp;&amp; y + sprite_height / 2 &lt;= bY + bHalfHeight {
    score++;
    
    randomize();
    x = scr_last_dumpling() + global.dumpling_spacing;
    y = irandom_range(sprite_height * 2, room_height - sprite_height * 2);
} else if x &lt; 0 {
    instance_create(0, 0, obj_game_over);
}

